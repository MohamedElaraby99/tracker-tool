{
  "name": "ackee",
  "private": true,
  "version": "3.5.1",
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "description": "Self-hosted, Node.js based analytics tool for those who care about privacy",
  "main": "src/index.js",
  "keywords": [
    "server",
    "tracking",
    "analytics"
  ],
  "license": "MIT",
  "homepage": "https://github.com/electerious/Ackee",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Ackee.git"
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/electerious"
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:pre": "cross-env BUILD_ENV=pre npm run build",
    "build": "node build.js",
    "server": "node src/index.js",
    "test": "npm run lint && ava",
    "lint": "eslint \"{functions,src,test}/**/*.js\""
  },
  "dependencies": {
    "@graphql-tools/merge": "^8.4.2",
    "ackee-tracker": "^5.1.1",
    "apollo-server-core": "^3.13.0",
    "apollo-server-lambda": "^3.13.0",
    "apollo-server-micro": "^3.13.0",
    "apollo-server-plugin-http-headers": "^0.1.4",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^1.3.8",
    "debounce-promise": "^3.1.2",
    "dotenv": "^16.6.1",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "is-url": "^1.2.4",
    "is-valid-domain": "^0.1.6",
    "micro": "^9.4.1",
    "microrouter": "^3.1.3",
    "mongoose": "^8.20.0",
    "node-fetch": "^2.7.0",
    "node-schedule": "^2.1.1",
    "normalize-url": "^6.1.0",
    "request-ip": "^3.3.0",
    "sanitize-filename": "^1.6.3",
    "signale": "^1.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.14.0",
    "@electerious/eslint-config": "^3.5.0",
    "ava": "^5.3.1",
    "classnames": "^2.5.1",
    "cross-env": "^10.1.0",
    "formbase": "^12.0.3",
    "history": "^5.3.0",
    "human-number": "^2.0.7",
    "mocked-env": "^1.3.5",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.1.11",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-apollo-network-status": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-fast-compare": "^3.2.2",
    "react-hotkeys-hook": "^3.4.7",
    "react-use": "^17.6.0",
    "rosid-handler-js-next": "^2.0.0",
    "rosid-handler-sass": "^9.0.0",
    "s-ago": "^2.2.0",
    "shortid": "^2.2.17",
    "test-listen": "^1.1.0",
    "url-pattern": "^1.0.3"
  },
  "ava": {
    "verbose": true,
    "timeout": "20s",
    "environmentVariables": {
      "ACKEE_TRACKER": "custom name"
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": "@electerious/eslint-config"
  },
  "nodemonConfig": {
    "exec": "npm run build:pre && npm run server",
    "ext": "js,json,graphql,scss",
    "watch": [
      "src"
    ]
  },
  "engines": {
    "node": ">= 14"
  }
}